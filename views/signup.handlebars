<div id="sup">
  <center>
            

            <div class="container">
                
            
            
            
            <div class="section"></div>
                <div class="z-depth-1 grey lighten-4 row"
                    style="display: inline-block; padding: 32px 48px 0px 48px; border: 1px solid #EEE;">

                    <form class="col s12" method="post">
    <form name="myForm" action="user/signup" method="post">

        <div class="container" id="mainsignup">
            <h3 class="main-title">Join collabHub</h3>
            <div class="colum1">
                <div class="row">
                    <div class="input-field col s12 m6">
                        First Name:
                        <input id="inputFirst" type="text" class="validate" name="firstName" required>
                        <label for="first_name"></label>
                    </div>
                    <div class="input-field col s12 m6">
                        Last Name:
                        <input id="inputLast" type="text" class="validate" name="lastName" required>
                        <label for="last_name"></label>
                    </div>
                    <div class="input-field col s12 m6">
                        City:
                        <input id="inputCity" type="text" class="validate" name="city" required>
                        <label for="city"></label>
                    </div>
                    <div class="input-field col s12 m6">
                        State:
                        <input id="inputState" type="text" class="validate" name="state" required>
                        <label for="state"></label>
                    </div>
                   
                </div>
            </div>
            <div class="colum">
                <div class="row">
                    <div class="input-field col s12 m6">
                        Zip:
                        <input id="inputZip" type="text" class="validate" name="zip" required>
                        <label for="zip"></label>
                    </div>
                    <div class="input-field col s12 m6">
                        GitHub username:
                        <input id="inputGitHub" type="text" class="validate" name="github" required>
                        <label for="github"></label>
                    </div>
                </div>
                
                <div class="row">
                     <div class="input-field col s12 m6">
                        Email:
                        <input id="inputEmail" type="text" class="validate" name="email" required>
                        <label for="email"></label>
                    </div>
                <div class="input-field col s12 m6">
                        Password:
                        <input id="inputPassword" type="password" class="validate" name="password" required>
                        <label for="password"></label>
                    </div>

                <div class="row">
                    <div class="input-field col s12">
                        {{!-- <input id="inputSkills" type="text" class="" name="skills" required> --}}
                        {{!-- <label for="skills">Add a skill</label> --}}
                 
                    <select id='profileSkills'> 
                      <option value="" disabled selected>Choose your skills</option>
                      <option value="Full Stack Developer">Full Stack Developer</option>
                      <option value="Back End Developer">Back End Developer</option>
                      <option value="Front End Developer">Front End Developer</option>
                      <option value="UXUI Designer">UXUI Designer</option>
                      <option value="Linux">Linux</option>
                      <option value="Android">Android</option>
                      <option value="iOS">iOS</option>           
                            </select>
                        </div>
                </div>
                        
                    <div class=" col s6">
                      Tell us something about yourself
                        <input id="inputBio"  type="textbox" style="font-size:15pt;height:200px;width:650px;"  name="Bio" required>
                        
                    </div>

                    
                </div>
                
            </div>
        </div>
         <center>
                          <div class="row">
                    <div class="col s12">
                    </div>
                    <input class="btn btn-round btn-large" id="add-account" type="submit" value="Submit">
                </div>
                        </center>
                    </form>
                </div>
                <div class="section"></div>
                <div class="section"></div>
    </form>
</div>
